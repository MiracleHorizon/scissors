{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [".env"],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".out/**"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^build",
        "^lint"
      ],
      "cache": false
    },
    "postbuild": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".out/**"
      ]
    }
  }
}
