{
  "name": "frontend-new",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:e2e": "NODE_ENV=test tsc && vite build",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:styles": "npx stylelint \"**/*.css\"",
    "format:styles": "npx stylelint --fix \"**/*.css\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "pnpm build:e2e && start-server-and-test start http://localhost:3000 cy:run",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check": "pnpm lint && pnpm test:coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
