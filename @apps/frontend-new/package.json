    "@radix-ui/react-form": "0.0.3",
    "@radix-ui/themes": "3.1.1",
    "@scissors/react-icons": "workspace:*",
    "@scissors/sharp": "workspace:*",
    "@scissors/utility": "workspace:*",
    "clsx": "2.1.1",
    "colorjs.io": "0.5.0",
    "cropperjs": "1.6.1",
    "dayjs": "1.11.11",
    "driver.js": "1.3.1",
    "exifr": "7.1.3",
