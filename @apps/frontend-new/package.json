    "react-compare-slider": "3.0.1",
    "react-day-picker": "8.10.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.5",
    "react-responsive": "10.0.0",
    "react-router": "7.1.1",
    "react-router-dom": "7.1.1",
    "sharp": "0.32.6",
    "sonner": "2.0.3",
    "vaul": "0.9.1",
